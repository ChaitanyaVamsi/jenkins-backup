    <script>pipeline {

    /*
     * Run on any available Jenkins agent
     * (Alternatively, you can restrict to a specific node â€“ see commented section below)
     */
    agent any

    /*
    // Example: Run on a specific Jenkins node
    agent {
        node {
            label &apos;slave&apos;
        }
    }
    */

    /*
     * Global environment variables
     * These can be used across all stages
     */
    environment {
        PROJECT   = &apos;roboshop&apos;
        COMPONENT = &apos;catalogue&apos;
    }

    /*
     * Pipeline-level options
     */
    options {
        // Abort the build if it runs longer than 10 minutes
        timeout(time: 10, unit: &apos;MINUTES&apos;)

        // Prevent multiple concurrent executions of this pipeline
        disableConcurrentBuilds()

        // Enable colored output in console logs
        ansiColor(&apos;xterm&apos;)
    }

    stages {

        /*
         * Initialize Terraform
         */
        stage(&apos;Init&apos;) {
            steps {
                withAWS(region: &apos;us-east-1&apos;, credentials: &apos;aws-creds&apos;) {
                    sh &apos;&apos;&apos;
                        cd 40-ecr
                        terraform init
                    &apos;&apos;&apos;
                }
            }
        }

        /*
         * Terraform plan stage
         * Shows what changes will be applied
         */
        stage(&apos;Plan&apos;) {
            steps {
                withAWS(region: &apos;us-east-1&apos;, credentials: &apos;aws-creds&apos;) {
                    sh &apos;&apos;&apos;
                        cd 40-ecr
                        terraform plan
                    &apos;&apos;&apos;
                }
            }
        }

        /*
         * Deployment stage
         * Requires manual approval before continuing
         */
        stage(&apos;Deploy&apos;) {

            /*
             * Manual approval step
             */
            input {
                message &apos;Should we continue with Terraform apply?&apos;
                ok &apos;Approve&apos;
                submitter &apos;alice,bob&apos;

                parameters {
                    text(
                        name: &apos;Approval Reason&apos;,
                        defaultValue: &apos;&apos;,
                        description: &apos;Reason for approving this Terraform apply&apos;
                    )
                }
            }

            steps {
                withAWS(region: &apos;us-east-1&apos;, credentials: &apos;aws-creds&apos;) {
                    sh &apos;&apos;&apos;
                        cd 40-ecr
                        terraform apply -auto-approve
                    &apos;&apos;&apos;
                }
            }
        }

    }
}
</script>
